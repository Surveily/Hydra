    "version": "0.2.0",
    "configurations": [
        {
            "name": "Copy Task",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Surveily.Hydra.Tools/bin/Debug/netcoreapp3.0/Surveily.Hydra.Tools.dll",
            "args": [
                "cp",
                "-s",
                "[1,2,3]",
                "[6,7,8]",
                "-t",
                "[4,5,6]"
            ],
            "cwd": "${workspaceFolder}/src/Surveily.Hydra.Tools",
            "console": "internalConsole",
            "stopAtEntry": false
        }
    ]
}